{
  "name": "farmvesty",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon ./app.js",
    "migratemysql": "db-migrate --config config/mysql/database.json --migrations-dir ./database/mysql/migrations",
    "migratepg": "db-migrate --config config/postgresql/database.json --migrations-dir ./database/postgresql/migrations",
    "migratemysql:up": "npm run migratemysql up",
    "migratepg:up": "yarn run migratepg up",
    "migratepg:create": "yarn run migratepg create",
    "migratemysql:create": "yarn run migratemysql create"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "cookie-session": "^2.0.0",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pg-promise": "^10.11.1",
    "sequelize": "^6.18.0",
    "supertest": "^6.2.2"
  }
}
